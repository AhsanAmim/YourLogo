<% layout('layouts/boilerplate')%>
<div id="cluster-map"></div>
<div class="container">
<h1>All Logodesigns</h1>

<% for (let logodesign of logodesigns){%>
<div class="card mb-3" >
    <div class="row">
        <div class="col-md-4">
            <%if(logodesign.images.length) {%>
            <img class="img-fluid" alt="" src="<%=logodesign.images[0].url%>">
            <% }else {%>
            <img class="img-fluid" alt=""
                src="https://res.cloudinary.com/douqbebwk/image/upload/v1600103881/YourLogo/lz8jjv2gyynjil7lswf4.png">
            <% } %>
        </div>
        <div class="col-md-8">
            <div class="card-body">
                <h5 class="card-title"><%= logodesign.title %> </h5>

                <p class="card-text"><%= logodesign.description %></p>
                <p class="card-text">
                    <small class="text-muted"><%= logodesign.logoconcept%></small>
                </p>
                <a class="btn btn-primary" href="/logodesigns/<%=logodesign._id%>">View <%=logodesign.title%></a>
            </div>
        </div>
    </div>
</div>
<% }%>
</div>